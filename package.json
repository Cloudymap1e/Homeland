{
  "name": "homeland-web-prototype",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node scripts/dev-server.mjs",
    "test": "node --test web/tests/*.test.mjs",
    "balance:sim": "node scripts/balance-sim.mjs --runs=1000 --maps=all --workers=auto --suite=full",
    "balance:verify": "node scripts/balance-sim.mjs --runs=1000 --maps=all --workers=auto --skip-search --suite=quick",
    "balance:diversity": "node scripts/balance-sim.mjs --runs=600 --maps=all --workers=auto --skip-search --suite=full",
    "balance:cuda-check": "node scripts/balance-sim.mjs --runs=400 --maps=all --workers=auto --cuda --suite=quick",
    "balance:gs75": "node scripts/balance-sim.mjs --runs=1000 --maps=all --workers=auto --cuda-required --suite=full",
    "tunnel:quick": "cloudflared tunnel --url http://localhost:4173",
    "tunnel:run": "cloudflared tunnel run homeland-web"
  }
}
