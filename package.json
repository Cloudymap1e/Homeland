{
  "name": "homeland-web-prototype",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node scripts/dev-server.mjs",
    "test": "node --test web/tests/*.test.mjs",
    "test:e2e": "npx --yes --package @playwright/test playwright test web/tests/slot-popout.e2e.spec.mjs --reporter=line",
    "balance:sim": "node scripts/balance-sim.mjs --runs=1000 --maps=all --workers=auto --suite=full --policies=random_all",
    "balance:verify": "node scripts/balance-sim.mjs --runs=1000 --maps=all --workers=auto --skip-search --suite=quick --policies=random_all",
    "balance:diversity": "node scripts/balance-sim.mjs --runs=600 --maps=all --workers=auto --skip-search --suite=full --skip-standard --policies=random_all",
    "balance:standard": "node scripts/balance-sim.mjs --runs=1000 --maps=all --workers=auto --skip-search --suite=criteria --policies=random_all",
    "balance:cuda-check": "node scripts/balance-sim.mjs --runs=400 --maps=all --workers=auto --cuda --suite=quick --policies=random_all",
    "balance:gs75": "node scripts/balance-sim.mjs --runs=1000 --maps=all --workers=auto --cuda-required --suite=full --policies=random_all",
    "tunnel:quick": "cloudflared tunnel --url http://localhost:4173",
    "tunnel:run": "./scripts/cloudflare-tunnel-run.sh"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2"
  }
}
